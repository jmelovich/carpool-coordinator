      {
        "id": "q3",
        "type": "short_text",
        "required": true,
        "question": "What is the make of the car (Honda, Mazda, etc...)?",
        "universal_id": "car_info[license_plate:<answer:q1>]@make"
      },
      {
        "id": "q4",
        "type": "short_text",
        "required": true,
